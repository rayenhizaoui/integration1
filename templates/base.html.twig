<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">
		<title>
			{% block title %}L'ESPORT
			{% endblock %}
		</title>
		<!-- Bootstrap core CSS -->
		<link href="{{ asset('bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
		{% block css %}
			<!-- Additional CSS Files -->
			<link rel="stylesheet" href="{{ asset('css/fontawesome.css') }}">
			<link rel="stylesheet" href="{{ asset('css/templatemo-lugx-gaming.css') }}">
			<link rel="stylesheet" href="{{ asset('css/owl.css') }}">
			<link rel="stylesheet" href="{{ asset('css/animate.css') }}">
			<link rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css">
		{% endblock %}
	</head>
	<body>
		<!-- ***** Preloader Start ***** -->
		<div id="js-preloader" class="js-preloader">
			<div class="preloader-inner">
				<span class="dot"></span>
				<div class="dots">
					<span></span>
					<span></span>
					<span></span>
				</div>
			</div>
		</div>
		<!-- ***** Preloader End ***** -->
		<script>
			// JavaScript pour cacher le préchargeur une fois que la page est chargée
window.addEventListener('load', function () {
var preloader = document.getElementById('js-preloader');
preloader.style.display = 'none';
});
		</script>

		<!-- ***** Header Area Start ***** -->
		{% block header %}
			<header class="header-area header-sticky">
				<div class="container">
					<div class="row">
						<div class="col-12">
							<nav
								class="main-nav">
								<!-- ***** Logo Start ***** -->
								<a href="index.html" class="logo">
									<img src="assets/images/logoo.png" alt="" style="width: 60px;">
								</a>
								<!-- ***** Logo End ***** -->
								<!-- ***** Menu Start ***** -->
								<ul class="nav">
									<li>
										<a href="index.html" class="active">Home</a>
									</li>
									<li>
										<a href="shop.html">Equipements</a>
									</li>
									<li>
										<a href="product-details.html">Paiement tournoi</a>
									</li>
									<li>
										<a href="contact.html">Evenements</a>
									</li>
									<li>
										<a href="contact.html">Equipes</a>
									</li>
									<li>
										<a href="{{ path('app_billet_add') }}">Billets</a>
									</li>
									<li>
										<a href="{{ path('app_user_add') }}">Inscription</a>
									</li>
									<li>
										<a href="{{ path('app_login') }}">Se connecter</a>
									</li>
								</ul>
								<a class='menu-trigger'>
									<span>Menu</span>
								</a>
								<!-- ***** Menu End ***** -->
							</nav>
						</div>
					</div>
				</div>
			</header>
		{% endblock %}
		<!-- ***** Header Area End ***** -->

		{% block content %}
			<!-- Page Content Goes Here -->


		{% endblock %}

		<footer>
			<div class="container">
				<div class="col-lg-12">
					<p>Copyright © 2048 L'ESPORT Company. All rights reserved.
						<a rel="nofollow" href="https://templatemo.com" target="_blank"></a>
					</p>
				</div>
			</div>
		</footer>

		<!-- Scripts -->
		<!-- Bootstrap core JavaScript -->
		{% block js %}
			<script src="{{asset('jquery/jquery.min.js')}}"></script>
			<script src="{{ asset('bootstrap/js/bootstrap.min.js')}}"></script>
			<script src="{{ asset('js/isotope.min.js') }}"></script>
			<script src="{{ asset('js/owl-carousel.js') }}"></script>
		{% endblock %}
	</body>
</html>
