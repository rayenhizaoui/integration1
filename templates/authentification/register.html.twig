{% block title %}Inscription
{% endblock %}

{% block body %}
	<h1>Inscription</h1>

	{{ form_start(form) }}
	{{ form_row(form.cin) }}
	{{ form_row(form.username) }}
	{{ form_row(form.numero) }}
	{{ form_row(form.email) }}
	{{ form_row(form.adresse) }}
	{{ form_row(form.password) }}

	<button type="submit" class="btn btn-primary">S'inscrire</button>
	{{ form_end(form) }}
{% endblock %}
